<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.colostate.cs.cs414.andyetitcompiles.p3.gui.RegisterDialogController">
   <children>
      <AnchorPane prefHeight="166.0" prefWidth="282.0">
         <children>
            <ButtonBar layoutX="37.0" layoutY="112.0" prefHeight="40.0" prefWidth="155.0">
              <buttons>
                  <Button fx:id="btnRegister" mnemonicParsing="false" onAction="#handleRegister" text="Register" />
                <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
              </buttons>
            </ButtonBar>
            <Label layoutX="12.0" layoutY="26.0" text="Nickname:" />
            <Label layoutX="14.0" layoutY="53.0" text="Email:" />
            <Label layoutX="14.0" layoutY="81.0" text="Password:" />
            <TextField fx:id="txtNickname" layoutX="84.0" layoutY="22.0" prefHeight="26.0" prefWidth="167.0" />
            <TextField fx:id="txtEmail" layoutX="84.0" layoutY="48.0" prefHeight="26.0" prefWidth="167.0" />
            <TextField fx:id="txtPassword" layoutX="84.0" layoutY="76.0" prefHeight="26.0" prefWidth="167.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
